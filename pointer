#include<stdio.h>

int main()
{

	int A[10];
	int * p ;
	p = &A[0];
	int n;
	
	for(int r=0;r<10;r++)
	{
	    	printf("\n Enter the number ");
	        scanf("%d",&n);
	        p[r] = n;
	}
	for(int k = 0;k<10;k++)
	{
	printf("\n Value of Array A[%d] = %d ",k,p[k]);
	}
	for(int l=0;l<10;l++)
	{
	    if(l%2 == 0)
	    {
	        printf("\n Value at even place is A[%d]= %d",l,p[l]);
	    }
	}
    	int e = p[0];
	    int y =p[1];
	    printf("\n Fibonnaci series is %d ",e);
     	printf("\n Fibonnaci series is %d ",y);
     	for(int h = 3; h<10; h++)
	{
	
	    int c = e + y;
		
		printf("\n Fibonnaci series is %d",c);
		e = y;
     	y = c;


   
	}
	
	
}
